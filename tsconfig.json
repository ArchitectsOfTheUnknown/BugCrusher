{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "outDir": "dist",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "strict": true,
    "noUnusedLocals": true
  },
  "include": [
    "index.ts",
	"OnMessage.ts",
	"data.json"
  ],
  "types": [
    "node"
  ],
  "exclude": [
    "node_modules"
  ]
}